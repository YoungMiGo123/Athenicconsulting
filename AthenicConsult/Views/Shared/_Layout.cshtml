<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Athenic Consulting</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"></script>
    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css?family=Quicksand:700|Roboto:400,400i,700&display=swap" rel="stylesheet">

    <!-- FontAwesome JS-->
    <script defer src="/athenictemplate/fontawesome/js/all.min.js"></script>

    <!-- Theme CSS -->
    <link id="theme-style" rel="stylesheet" href="/athenictemplate/css/theme.css">
    <link rel="stylesheet" href="/css/site.css" />

</head>
<body>

    @{
        await Html.RenderPartialAsync("/Views/Shared/Components/_Nav.cshtml");
    }
    <div class="container" style="min-height: 60vh;">
        <main class="main-content position-relative max-height-vh-100 h-100 border-radius-lg ">
            @RenderBody()
        </main>
    </div>

    @{
        await Html.RenderPartialAsync("/Views/Shared/Components/_Footer.cshtml", new LeadViewModel() { Email = string.Empty });
    }
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <!-- Javascript -->
    <script src="/athenictemplate/plugins/popper.min.js"></script>
    <script src="/athenictemplate/plugins/bootstrap/js/bootstrap.min.js"></script>
    <script src="/athenictemplate/plugins/smoothscroll.min.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
